{
  "manifest_version": 3,
  "name": "user journey",
  "version": "0.0.1",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "record.png"
  },
  "permissions": [
    "tabs"
  ],
"content_scripts": [
  {
    "matches": ["https://*/*"],
    "js": ["content-script.js"]
  }
],
"background": {
  "service_worker": "background.js"
}
}